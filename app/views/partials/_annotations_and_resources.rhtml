    <div id="annotations">
      <%=
       render( :partial => "partials/annotation_group", :collection => (Resource.annotation_types - [:images, :urls, :blogs, :videos] ) )
      %> 
    </div>
    <br>
    <div id="resources">
      <%= render( :partial => "partials/resource", :collection => @resources ) %>
    </div>
    
    <% if @resources.empty? %>
       <b style="display:block; margin: 40px auto; width:200px;">Sorry, nothing found!</b>
    <% end %>
